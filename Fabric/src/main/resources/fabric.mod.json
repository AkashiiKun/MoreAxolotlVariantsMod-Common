    "id": "${mod_id}",
    "version": "${mod_version}",
  
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
      "${mod_author}"
    ],
    "license": "${license}",
    "icon": "icon.png",
  
    "environment": "*",
    "entrypoints": {
      "init": [
        "io.github.akashiikun.mavm.MoreAxolotlVariantsMod"
