      ]
    },
    "mixins": [
      "${mod_id}.mixins.json"
    ],
    "depends": {
      "fabricloader": ">=0.13",
      "fabric": "*",
      "minecraft": "1.18.x",
      "java": ">=17"
    },
  "custom": {
    "mc-publish": {
      "modrinth": "zitu8weW",
      "curseforge": "498797"
    }
  }
}
  