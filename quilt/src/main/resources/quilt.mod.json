{
  "schema_version": 1,
  "mixin": [
    "${mod_id}.mixins.json"
  ],
  "quilt_loader": {
    "group": "${maven_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": ["${mod_author}"],
    "license": "${license}",
    "icon": "icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "io.github.akashiikun.mavm.MoreAxolotlVariantsModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      }
    ]
  }
}